{
    "name": "recca0120/parallel-test",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "recca0120",
            "email": "recca0120@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "ext-json": "*",
        "guzzlehttp/promises": "^1.4",
        "guzzlehttp/psr7": "^1.8",
        "symfony/http-foundation": "^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9|^1.3",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.18",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Recca0120\\ParallelTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Recca0120\\ParallelTest\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Recca0120\\ParallelTest\\ParallelTestServiceProvider"
            ]
        }
    }
}
