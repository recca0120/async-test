{
    "name": "recca0120/laravel-parallel",
    "description": "",
    "keywords": [
        "laravel",
        "testing",
        "parallel",
        "parallel request"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "recca0120",
            "email": "recca0120@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "ext-json": "*",
        "amphp/process": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "^0.9|^1.3",
        "orchestra/testbench": "^5.19|^6.18|^7.0",
        "phpunit/phpunit": "^8.5|^9.1"
    },
    "autoload": {
        "psr-4": {
            "Recca0120\\LaravelParallel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Recca0120\\LaravelParallel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Recca0120\\LaravelParallel\\ParallelServiceProvider"
            ]
        }
    }
}
